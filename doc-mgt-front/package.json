{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "test:update-snap": "jest --updateSnapshot",
    "generate": "graphql-codegen",
    "codegen": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen --config codegen.ts",
    "openapi:generate": "openapi-generator-cli generate -g typescript-angular -o ./src/services/generated/ -i openapi.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@apollo/client": "^3.0.0",
    "apollo-angular": "^7.0.2",
    "graphql": "^16",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^18.0.0",
    "@angular-devkit/build-angular": "^18.0.7",
    "@angular/cli": "^18.0.7",
    "@angular/compiler-cli": "^18.0.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript-apollo-angular": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/jest": "^29.5.12",
    "cross-env": "^7.0.3",
    "graphql": "^16.9.0",
    "jasmine-core": "~5.1.0",
    "jest": "^29.7.0",
    "typescript": "~5.4.2"
  }
}
